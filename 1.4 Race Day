let raceNumber = Math.floor(Math.random() * 1000);

const earlyRunner = true;
const runnerAge = 44;

if (runnerAge > 18 && earlyRunner) {
  raceNumber+= 1000
}

if (runnerAge > 18 && earlyRunner) {
  console.log(raceNumber + ' You will race at 9:30 am.');
} else if (runnerAge > 18 && !earlyRunner) {
  console.log(raceNumber + ' You will race at 11:00 am.');
} else if (runnerAge < 18) {
  console.log(raceNumber + ' You will race at 12:30 pm.');
} else {
  console.log('Please see the registration desk for information.');
}

