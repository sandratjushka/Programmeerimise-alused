nimi = prompt('Sisestage oma nimi: ');
lubatud_kiirus = prompt('Sisestage lubatud kiirus (km/h): ');
tegelik_kiirus = prompt('Sisestage tegelik kiirus (km/h): ');
let vastus = ''

function trahv(lubatud_kiirus, tegelik_kiirus) {
vastus = 3*(tegelik_kiirus - lubatud_kiirus);
if (vastus <= 190) {
vastus == vastus;
console.log(nimi + ', teie trahv kiiruse Ã¼letamise eest on ' + vastus + ' eurot.');
} else {
console.log(nimi + ', teie trahv kiiruse Ã¼letamise eest on 190 eurot.')
}
}

trahv(lubatud_kiirus, tegelik_kiirus);
